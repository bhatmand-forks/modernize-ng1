{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 10",
            "ie 11"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "syntax-decorators",
    "ng-annotate"
  ]
}